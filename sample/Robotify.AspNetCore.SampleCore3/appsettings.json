{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Robotify2": {
    "Enabled": true,
    "SitemapUrl": "https://www.example.com/sitemap.xml",
    "CrawDelay": 10,
    "DisableFileHeaderComments": true,
    "Groups": [
      {
        "UserAgent": "*",
        "Disallow": [ "/" ]
      },
      {
        "UserAgent": "Googlebot",
        "Disallow": [ "/admin" ]
      },
      {
        "UserAgent": "AnotherBot",
        "Allow": [ "/search" ]
      }
    ]
  }
}
